package net.picklez.as.exercises;

/**
 * @author Ben Pickering
 * @since 5 Nov 2015 | 11:01:26
 */

public class ArmstrongNumbers {

	public static void main(String[] args) {
		
		int num = 4;
		
		int fac = 0;
		for (int i = 1; i <= num; i++) {
			if (fac == 0)
				fac = i;
			else
				fac *= i;
		}
		
		System.out.println(fac);
		
	}
	
}