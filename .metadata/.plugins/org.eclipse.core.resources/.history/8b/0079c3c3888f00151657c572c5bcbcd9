package net.picklez.utils.queues;

/**
 * @author Ben Pickering
 * @since 20 Nov 2015 | 13:11:31
 */

public class Queue {

	private int maxSize;
	private Object[] objects;
	
	public Queue(Object obj) {
		this.objects = new Object[] { obj };
	}
	
	public Queue() {
		
	}
	
	public void addItem(Object o) {
		if (this.objects.length < maxSize)
		this.objects[this.objects.length] = 0;
		else
			System.out.println("Failed to add object: ");
	}
}