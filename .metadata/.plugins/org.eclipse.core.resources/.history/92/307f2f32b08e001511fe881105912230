public class LinearSearch {

	public static void main(String[] args) {
		int num = 5;
		
		System.out.println("Looking for number: " + num);
int[] array = new int[1000];
		
		for (int i = 0; i < array.length; i++) {
			array[i] = (int)(Math.random() * array.length);
		}
	}
	
}