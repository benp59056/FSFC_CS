package net.picklez.as.exercises.arrays;

import net.picklez.utils.StringUtils;

/**
 * @author Ben Pickering
 * @since 12 Nov 2015 | 11:05:57
 */

public class ReversingArrays {

	public static void main(String[] args) {
		String[] original = {"1", "2", "3", "4", "5"};
		String[] reversed = StringUtils.reverseArray(original);
		
		for (String s : reversed) {
			System.out.println(s);
		}
	}
	
}