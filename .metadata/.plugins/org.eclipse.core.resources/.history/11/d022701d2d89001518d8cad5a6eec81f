package net.picklez.as.exercises;

/**
 * @author Ben Pickering
 * @since 20 Oct 2015 | 09:15:18
 */

public class SubtractingBy5 {

	public static void main(String[] args) {
		int start = 125;
		int num = 5;
		int min = 5;
		
		for (int i = start; i >= min; i-=num) {
			System.out.println(i);
		}
	}
	
}
