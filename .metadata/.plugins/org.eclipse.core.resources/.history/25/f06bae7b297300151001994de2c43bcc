package net.picklez.as.exercises;

/**
 * @author Ben Pickering
 * @since 15 Oct 2015
 */

public class Loop {

	public static void main(String[] args) {
		int var = 0;
		
		if (var <= 0) {
			System.out.println("Less than or equal to 0");
		}
		
		for (int i = 0; i < 50; i++) {
			if (i > 30) {
				System.out.println(i + " is bigger than 30");
			}
		}
		
		int i = 0;
		
		while (i < 30) {
			
		}
	}
	
}
